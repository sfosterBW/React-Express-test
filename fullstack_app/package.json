{
  "name": "fullstack_app",
  "version": "1.0.0",
  "description": "React and express app to try things out",
  "main": "index.js",
  "dependencies": {
    "axios": "^0.19.0",
    "create-react-app": "^3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "concurrently": "^3.5.1",
    "cypress": "^4.1.0",
    "redux-devtools-extension": "^2.13.8"
  },
  "scripts": {
    "client": "cd client && npm start",
    "api": "cd api && npm run start",
    "api:test": "cd api && npm run start:test",
    "client:dev": "cd client && npm start",
    "api:dev": "cd api && npm run dev",
    "start": "concurrently --kill-others \"npm run api\" \"npm run client\"",
    "dev": "concurrently --kill-others \"npm run api:dev\" \"npm run client:dev\"",
    "test": "concurrently --kill-others \"npm run api:test\" \"npm run client\" \"./node_modules/.bin/cypress run\"",
    "cypress:open": "cypress open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
